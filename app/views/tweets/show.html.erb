<p id="notice"><%= notice %></p>

<div class="container">
  <p>
  <strong>Foto:</strong>
  <img src="<%= @tweet.user.profile_photo %>" width="60" height="60" class="rounded-circle mb-2">
  </p>

  <p>
  <strong>Nombre:</strong>
  <%= @tweet.user.username %>
  </p>

  <p>
    <strong>Content:</strong>
    <%= @tweet.content %>
  </p>


  <div class="container">
    <p>liked by:</p>

    <% @tweet.votes_for.voters.each do | p | %> 
      <img src="<%= p.profile_photo %>" width="60" height="60" class="round-circle mb-2" >
    <% end%>


  </div>


</div>

  <%= link_to 'Back', tweets_path %>
